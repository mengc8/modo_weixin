.right-item {
  padding: 22rpx 26rpx 22rpx 90rpx;
  background: #f5f5f5;
  box-shadow: 0 0 2rpx 4rpx rgba(0, 0, 0, 0.06);
  border-radius: 6rpx;
  margin-bottom: 28rpx;
}

.right-item-title {
  font-size: 26rpx;
  margin-bottom: 40rpx;
  position: relative;
  font-weight: 600;
}

.right-item-title::before, .right_item_type0::after, .right_item_type1::after,
.right_item_type2::after {
  display: inline-block;
  content: "";
  position: absolute;
}

.right-item-title::before {
  width: 42rpx;
  height: 42rpx;
  background: #333;
  border-radius: 21rpx;
  left: -62rpx;
  top: -4rpx;
}

.right_item_type0::after {
  width: 27rpx;
  height: 29rpx;
  background: url(data:image/png;base64,);
  left: -52rpx;
  top: 2rpx;
  background-size: cover;
}

.right_item_type1::after {
  width: 32rpx;
  height: 32rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  left: -56.1rpx;
  top: 2rpx;
}

.right_item_type2::after {
  content: "免";
  left: -53.1rpx;
  top: -1rpx;
  color: #fff;
  font-weight: 300;
  font-size: 26rpx;
}

.right-item-content {
  font-size: 24rpx;
  line-height: 42rpx;
}

.btn-exchage {
  width: 680rpx;
  height: 80rpx;
  background: #f64;
  border-radius: 0rpx;
  margin: 120rpx auto;
  bottom: 20rpx;
  color: #fff;
  font-size: 34rpx;
  line-height: 80rpx;
  text-align: center;
  box-shadow: 0 2px 2px 0px rgba(255, 126, 91, 0.3);
}
.font26{
  font-size: 26rpx;
  font-weight: 700;
}


