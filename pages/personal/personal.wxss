.person-header {
  padding: 54rpx 0 98rpx;
  color: #666;
  text-align: center;
  height: 392rpx;
  position: relative;
  box-sizing: border-box;
}

.bg-img {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: -1;
}

.head-img {
  height: 128rpx;
  width: 128rpx;
  border-radius: 64rpx;
  margin: 0 auto;
  background: #eee;
}

.person-note {
  font-size: 30rpx;
  color: #fff;
  margin-top: 6rpx;
}
.person-main-box{
  margin: -50rpx 30rpx;
  box-shadow: 6rpx 6rpx 12rpx rgba(0, 0, 0, 0.1) ,6rpx -6rpx 12rpx rgba(0, 0, 0, 0.1),-6rpx 6rpx 12rpx rgba(0, 0, 0, 0.1);
  display: flex;
  text-align: center;
  background: #fff;
}
.main-box-item{
  width: 230rpx;
  padding: 42rpx 0;
}
.icon-dai{
  width: 57rpx;
  height: 39rpx;
  margin: 7rpx 0 4rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}
.icon-score{
  width: 48rpx;
  height: 48rpx;
  margin: 1rpx 0 2rpx;
background: url(data:image/png;base64,);
background-size: cover;
}
.icon-storage{
  width: 50rpx;
  height: 50rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}
.item-title{
  font-size: 24rpx;
  margin: 4rpx 0 4rpx;
}
.item-content{
  font-size: 20rpx;
  color: #999;
}
.img-box{
  margin: 90rpx 30rpx 0;
  height: 168rpx;
  position: relative;
  color: #fff;
  font-size: 22rpx;
  padding: 46rpx 0 0 210rpx;
  box-sizing: border-box;
}
.img-box-img{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.open-vip{
  font-size: 28rpx;
  margin-bottom: 6rpx;

}
.more-vip1{
  display: inline-block;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
/* .more-vip:hover{
color: #ff6644;
display: inline-block;
} */
.one-item{
  margin: 0 30rpx;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 30rpx;
}
.one-item-right{
  font-size: 24rpx;
  color: #999;
  position: relative;
  padding-right: 30rpx;
  box-sizing: border-box;
}
.one-item-right::after{
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-top: 1rpx solid #999;
  border-right: 1rpx solid #999;
  transform: rotate(45deg);
  position: absolute;
  right: 0rpx;
  top: 55rpx;
  content: "";
}
.vip_huang,.vip_bo,.vip_zuan{
  display: inline-block;
  width: 106rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 22rpx;
  text-align: center;
  background: linear-gradient(to right,#ff4d4d,#ff6644);
  border-radius: 18rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}